---
import BaseLayout from '../layouts/Base.astro';

const response = await fetch(`https://florian.geierstanger.org/lib/exe/ajax.php?call=api&fn=page&id=filmvermittlung`);
const page = await response.json();

---
<BaseLayout title="{page.title}" hasTitleElement={true}>
	<div>{page.html}</div>
</BaseLayout>
